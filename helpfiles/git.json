{
    "name": "git",
    "synonyms": "github",
    "commands":[
        {
            "command": "git status",
            "description": "Get status for repository"
        },
        {
            "command": "git config --global user.name=<your_username>",
            "description": "Set username for commits from this machine"
        },
        {
            "command": "ssh -T git@bitbucket.org",
            "description": "Check your SSH connection to Bitbucket"
        },
        {
            "command": "ssh -T git@github.org",
            "description": "Check your SSH connection to Github"
        },
        {
            "command": "git push --set-upstream origin master",
            "description": "First push when creating your github repo locally"
        }
    ]
}